pipeline {
    agent any 
    stages {
        stage('code from scm') {
            steps {
                git credentialsId: 'gitkey', url: 'git@github.com:gopal1409/mass-june-test-25.git'
            }
        }
        stage('mvn build') {
            steps {
                //get build
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('mvn test') {
            steps {
                //lets test the code 
                sh 'mvn test'
            }
        }
    }
}
